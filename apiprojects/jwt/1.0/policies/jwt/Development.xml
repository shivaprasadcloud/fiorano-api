<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<jsonWebTokenConfiguration policyName="JSON_WEB_TOKEN" policyType="SECURITY">
    <policyInstanceName>jwt</policyInstanceName>
    <additionalClaim>
        <entry>
            <key>name</key>
            <value>
                <defaultValue>default</defaultValue>
                <name>yui</name>
                <type>CONTEXT_VARIABLE</type>
            </value>
        </entry>
    </additionalClaim>
    <alg>
        <defaultValue>default</defaultValue>
        <name></name>
        <type>CONSTANT</type>
    </alg>
    <aud>
        <defaultValue>default</defaultValue>
        <name></name>
        <type>CONSTANT</type>
    </aud>
    <exp>600000</exp>
    <iss>
        <defaultValue>default</defaultValue>
        <name></name>
        <type>CONSTANT</type>
    </iss>
    <jsonWebKeys>
        <defaultValue>default</defaultValue>
        <name></name>
        <type>CONSTANT</type>
    </jsonWebKeys>
    <keysURL>
        <defaultValue>default</defaultValue>
        <name></name>
        <type>CONSTANT</type>
    </keysURL>
    <loadKeysFromURL>false</loadKeysFromURL>
    <secretKey>
        <defaultValue>default</defaultValue>
        <name></name>
        <type>CONSTANT</type>
    </secretKey>
    <show>
        <defaultValue>default</defaultValue>
        <name></name>
        <type>CONSTANT</type>
    </show>
    <sub>
        <defaultValue>default</defaultValue>
        <name></name>
        <type>CONSTANT</type>
    </sub>
    <units>SECONDS</units>
    <kID>
        <defaultValue>default</defaultValue>
        <name></name>
        <type>CONSTANT</type>
    </kID>
</jsonWebTokenConfiguration>
